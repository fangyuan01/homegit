package com.cwx.max.bean;

import java.util.List;

public class UserDetail {
	private String userImg;
	private String name;
	private String id;
	private String createTime;
	private String userCountry;
	private List<UserHero> usuallyHeros;
	private List<UserMatch> lateMatchs;
	private List<UserRecord> userRecords;

	public UserDetail() {
	}

	public UserDetail(String userImg, String name, String id, String createTime, String userCountry, List<UserHero> usuallyHeros,
			List<UserMatch> lateMatchs, List<UserRecord> userRecords) {
		this.userImg = userImg;
		this.name = name;
		this.id = id;
		this.createTime = createTime;
		this.userCountry = userCountry;
		this.usuallyHeros = usuallyHeros;
		this.lateMatchs = lateMatchs;
		this.userRecords = userRecords;
	}

	public String getName() {
		return name;
	}

	public String getId() {
		return id;
	}

	public String getCreateTime() {
		return createTime;
	}

	public List<UserHero> getUsuallyHeros() {
		return usuallyHeros;
	}

	public List<UserMatch> getLateMatchs() {
		return lateMatchs;
	}

	public List<UserRecord> getUserRecords() {
		return userRecords;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setId(String id) {
		this.id = id;
	}

	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}

	public void setUsuallyHeros(List<UserHero> usuallyHeros) {
		this.usuallyHeros = usuallyHeros;
	}

	public void setLateMatchs(List<UserMatch> lateMatchs) {
		this.lateMatchs = lateMatchs;
	}

	public void setUserRecords(List<UserRecord> userRecords) {
		this.userRecords = userRecords;
	}

	public String getUserImg() {
		return userImg;
	}

	public void setUserImg(String userImg) {
		this.userImg = userImg;
	}

	public String getUserCountry() {
		return userCountry;
	}

	public void setUserCountry(String userCountry) {
		this.userCountry = userCountry;
	}

}
